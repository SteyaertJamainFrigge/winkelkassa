<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="FriggeSteyaertJamain.be.winkelKassa.ui.gui.MainController">
    <MenuBar VBox.vgrow="NEVER" fx:id="menubar">
        <Menu mnemonicParsing="false" text="Bestand">
            <MenuItem fx:id="quitItem" mnemonicParsing="false" onAction="#quit" text="Quit"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Bewerken">
            <MenuItem fx:id="ClientItem" mnemonicParsing="false" onAction="#startClientWindow" text="Klanten"/>
            <MenuItem fx:id="supliersItem" mnemonicParsing="false" onAction="#startSuppliersScene" text="Leveranciers"/>
            <MenuItem fx:id="productItem" mnemonicParsing="false" onAction="#startProductWindow" text="Producten"/>
            <MenuItem fx:id="packagingItem" mnemonicParsing="false" onAction="#startPagagingScene" text="Leeggoed"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Rapporten">
            <MenuItem fx:id="transactionsItem" mnemonicParsing="false" text="Verrichtingen"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Extra">
            <MenuItem fx:id="usersItem" mnemonicParsing="false" text="Gebruikers"/>
            <MenuItem fx:id="exportItem" mnemonicParsing="false" text="Export"/>
            <MenuItem fx:id="optionsItem" mnemonicParsing="false" text="Instellingen"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Login">
            <MenuItem fx:id="action1Item" mnemonicParsing="false" text="Action 1"/>
        </Menu>
    </MenuBar>
    <AnchorPane prefHeight="414.0" prefWidth="649.0" VBox.vgrow="ALWAYS">
        <Button fx:id="showRegisterBtn" layoutX="272.0" layoutY="133.0" mnemonicParsing="false"
                onAction="#ShowRegister" prefHeight="89.0" prefWidth="109.0" text="Kassa"/>
    </AnchorPane>
</VBox>
